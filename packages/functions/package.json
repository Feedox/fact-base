{
	"name": "@proj/functions",
	"version": "0.0.1",
	"main": "build/index.js",
	"private": true,
	"engines": {
		"node": "v16"
	},
	"type": "module",
	"scripts": {
		"clean": "rm -rf build",
		"build": "yarn clean; tsc -b",
		"watch": "yarn clean; tsc -b -w",
		"start": "tsx -r dotenv/config src/test.ts",
		"serve": "nodemon --watch 'src/**/*.ts' --exec tsx --inspect -r dotenv/config src/index.ts",
		"functions:serve": "nodemon --watch 'build/**/*.js' --exec functions-framework --target=api",
		"functions:debug": "nodemon --watch 'build/**/*.js' --exec node --inspect node_modules/.bin/functions-framework --target=api",
		"functions:deploy": "gcloud functions deploy altgpt-plugins --runtime=nodejs16 --trigger-http --entry-point=api --allow-unauthenticated"
	},
	"dependencies": {
		"libx.js": "^2.35.0"
	},
	"devDependencies": {
		"@google-cloud/functions-framework": "^3.1.3",
		"@types/express": "^4.17.17",
		"@types/node": "^18.15.10",
		"concurrently": "^7.6.0",
		"cors": "^2.8.5",
		"jest": "^29.5.0",
		"nodemon": "^2.0.22",
		"tsx": "^3.12.6",
		"typeorm": "^0.3.12",
		"typescript": "^5.0.2"
	},
	"workspaces": {
		"nohoist": [
			"/@proj",
			"**/@proj/",
			"**/@proj/**",
			"**/@types/**",
			"**"
		]
	}
}
